package exception;

import java.util.Scanner;

public class Ex01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num;
		
		System.out.print("정수 입력: ");
		num = sc.nextInt();
		
		System.out.println("입력 받은 값: " + num);
		
		sc.close();
	}

}
