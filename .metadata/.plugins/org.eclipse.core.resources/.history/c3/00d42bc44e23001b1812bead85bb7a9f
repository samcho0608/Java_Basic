package exception;

public class Ex03 {

	public static void printArr(int[] arr) {
		for(int a : arr)
			System.out.print(a + " ");
		System.out.println();
		
		throw ArrayIndexOutOfBoundsException("배열 예외");
	}
	
	public static void main(String[] args) {
		int[] arr = new int[] {10, 20, 30, 40, 50};
		
		printArr(arr);
	}

}
