package _interface;

public class Ex04 {

	public static void main(String[] args) {
		Slash sl = new Slash() {
			
			@Override
			public void slash(String weapon) {
				System.out.println(weapon + "·Î º£´Ù.");
			}
		};

		sl.slash("Ä®");
		
		// lambda function
		Slash sl2 = (String weapon) -> {
			System.out.println(weapon + "·Î º£´Ù.");
		};
		
		sl2.slash("µµ³¢");
	}

}
