package exception;

public class Ex04 {

	public static void main(String[] args) throws InterruptedException {

		try {
			for(int i = 0; i < 10; i++) {
				System.out.println(i + "ÃÊ~");
				Thread.sleep(1000);
			}
		}
		catch(InterruptedException e) {
			e.printStackTrace();
		}

	}

}
