package _interface;

public class Ex04 {

	public static void main(String[] args) {
		Slash sl = new Slash() {
			
			@Override
			public void slash() {
				System.out.println("Ä®·Î º£´Ù.");
			}
		};

		sl.slash();
		
		// lambda function
		Slash sl2 = () -> {
			System.out.println("µµ³¢·Î º£´Ù.");
		};
		
		sl2.slash();
	}

}
