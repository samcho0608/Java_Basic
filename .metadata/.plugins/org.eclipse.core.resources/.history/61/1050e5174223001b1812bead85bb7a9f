package nestedClass;

class Local{
	private int out = 10;
	
	void showRun() {
		
		class Inner2{	// class like a local variable, once out of the scope it is no longer usable.
			private int in = 20;
			
			void inFunc() {
				System.out.println("=== 지역 내부 클래스 ===");
				System.out.println("외부 멤버 : " + out);
				System.out.println("내부 멤버 : " + in);
			}
			
		}
	}
}

public class Ex03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
